<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<jdbcConnection connectionURL="jdbc:mysql://localhost:3306/youhua?useUnicode=true&amp;characterEncoding=utf8&amp;useSSL=false"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />
		<javaModelGenerator targetPackage="mapping"
			targetProject="GeneratorMySQL" />
		<sqlMapGenerator targetPackage="mapping"
			targetProject="GeneratorMySQL" />
		<javaClientGenerator targetPackage="mapping"
			targetProject="GeneratorMySQL" type="XMLMAPPER" />
		<table schema="youhua" tableName="userinfo">
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
		</table>
	</context>
</generatorConfiguration>